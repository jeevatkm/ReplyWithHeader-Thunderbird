{
    "manifest_version": 2,
    "name": "ReplyWithHeaderDev",
    "description": "Outlook style headers and few goodies for Thunderbird",
    "version": "3.0.0",
    "author": "Jeevanandam M.",
    "homepage_url": "https://myjeeva.com/replywithheader-mozilla",
    "browser_specific_settings": {
        "gecko": {
            "id": "replywithheader-dev@myjeeva.com",
            "strict_min_version": "115.0"
        }
    },
    "icons": {
        "16": "images/rwh.svg",
        "24": "images/rwh.svg",
        "32": "images/rwh.svg",
        "64": "images/rwh.svg",
        "128": "images/rwh.svg"
    },
    "permissions": [
        "storage",
        "notifications",
        "menus",
        "compose",
        "messagesRead"
    ],
    "background": {
        "page": "background.html"
    },
    "options_ui": {
        "page": "/options/options.html"
    }
}
